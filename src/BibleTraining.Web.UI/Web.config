<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
  </configSections>
  <connectionStrings>
    <add name="BibleTrainingDomain" connectionString="Server=.;trusted_connection=true;Initial Catalog=BibleTraining" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <appSettings>
    <add key="BaseUrl" value="/BibleTraining/" /> <!-- For local dev -->
    <add key="DataApiBaseUrl" value="/BibleTraining" />
    <add key="Locale" value="en-US" />
    <add key="ViewFolder" value="dist" />
  </appSettings>
  <system.web>
    <authentication mode="None" />
    <compilation debug="true" targetFramework="4.6.1" />
    <httpRuntime targetFramework="4.6" />
  </system.web>
  <system.webServer>
    <modules>
      <remove name="FormsAuthentication" />
    </modules>

    <staticContent>
      <remove fileExtension=".woff" />
      <mimeMap fileExtension=".woff" mimeType="application/x-font-woff" />
      <remove fileExtension=".woff2" />
      <mimeMap fileExtension=".woff2" mimeType="application/x-font-woff" />
      <remove fileExtension=".ttf" />
      <mimeMap fileExtension=".ttf" mimeType="font/truetype" />
      <remove fileExtension=".otf" />
      <mimeMap fileExtension=".otf" mimeType="font/opentype" />
      <remove fileExtension=".eot" />
      <mimeMap fileExtension=".eot" mimeType="application/vnd.ms-fontobject" />
      <remove fileExtension=".mp4" />
      <mimeMap fileExtension=".mp4" mimeType="video/mp4" />
      <remove fileExtension=".ogv" />
      <mimeMap fileExtension=".ogv" mimeType="video/ogv" />
      <remove fileExtension=".webm" />
      <mimeMap fileExtension=".webm" mimeType="video/webm" />
      <remove fileExtension=".svg" />
      <mimeMap fileExtension=".svg" mimeType="image/svg+xml" />
      <remove fileExtension=".json" />
      <mimeMap fileExtension=".json" mimeType="application/json" />
    </staticContent>
    <rewrite>
      <rules>
        <!-- Development settings -->
        <rule name="look in built first" stopProcessing="true">
          <match url="^(.+)$" />
          <conditions>
            <add input="{APPL_PHYSICAL_PATH}built\{R:1}" matchType="IsFile" />
          </conditions>
          <action type="Rewrite" url="built/{R:1}" />
        </rule>
        <rule name="look in src next" stopProcessing="true">
          <match url="^(.+)$" />
          <conditions>
            <add input="{APPL_PHYSICAL_PATH}src\{R:1}" matchType="IsFile" />
          </conditions>
          <action type="Rewrite" url="src/{R:1}" />
        </rule>
        <!-- Production Settings
        <rule name="serve from dist" stopProcessing="true">
            <match url="^(.+)$" />
            <conditions>
                <add input="{APPL_PHYSICAL_PATH}dist\{R:1}" matchType="IsFile" />
            </conditions>
            <action type="Rewrite" url="dist/{R:1}" />
        </rule> -->
      </rules>
    </rewrite>
  <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
      <remove name="TRACEVerbHandler" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
    </handlers></system.webServer>
